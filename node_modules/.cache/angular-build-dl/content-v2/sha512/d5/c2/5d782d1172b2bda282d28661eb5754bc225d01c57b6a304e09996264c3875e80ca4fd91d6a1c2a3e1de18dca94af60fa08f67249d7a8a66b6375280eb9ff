    var AuthService = /*#__PURE__*/function () {
      function AuthService(router) {
        _classCallCheck(this, AuthService);

        this.router = router;
        this.isAuthenticate = false;
        this.readFromLocalStorage();
      }

      _createClass(AuthService, [{
        key: "login",
        value: function login(user, password) {
          if (user === 'admin' && password === 'admin') {
            this.isAuthenticate = true;
            this.writeToLocalStorage();
            return this.isAuthenticate;
          }

          this.isAuthenticate = false;
          return this.isAuthenticate;
        }
      }, {
        key: "logout",
        value: function logout() {
          if (this.isLoggedIn()) {
            this.isAuthenticate = false;
            this.writeToLocalStorage();
            this.goToHomePage();
            return true;
          } else {
            return false;
          }
        }
      }, {
        key: "isLoggedIn",
        value: function isLoggedIn() {
          this.readFromLocalStorage();
          return this.isAuthenticate;
        }
      }, {
        key: "writeToLocalStorage",
        value: function writeToLocalStorage() {
          localStorage.setItem('auth', '' + this.isAuthenticate);
        }
      }, {
        key: "readFromLocalStorage",
        value: function readFromLocalStorage() {
          this.isAuthenticate = localStorage.getItem('auth') === 'true';
        }
      }, {
        key: "goToHomePage",
        value: function goToHomePage() {
          this.router.navigate(['']);
        }
      }, {
        key: "goToLoginPage",
        value: function goToLoginPage() {
          this.router.navigate(['login']);
        }
      }]);

      return AuthService;
    }();

